{% sw_extends '@Storefront/storefront/layout/meta.html.twig' %}



        {% block layout_head_stylesheet %}
            <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300"
                  rel="stylesheet"
                  type="text/css">
            <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900&display=swap"
                  rel="stylesheet">
            {{ parent() }}
        {% endblock %}


        {% block layout_head_meta_tags_general %}
            <meta name="author"
                  content="{{ metaInformation.author|striptags }}" />
            <meta name="robots"
                  content="{% block layout_head_meta_tags_robots %} noindex,nofollow {% endblock %}" />
            <meta name="revisit-after"
                  content="{{ metaInformation.revisit|striptags }}" />
            <meta name="keywords"
                  content="{% block layout_head_meta_tags_keywords %}{{ metaKeywords }}{% endblock %}" />
            <meta name="description"
                  content="{% block layout_head_meta_tags_description %}{{ metaDescription }}{% endblock %}" />
        {% endblock %}
